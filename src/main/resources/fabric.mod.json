{
	"schemaVersion": 1,
	"id": "mod-client-or-server",
	"version": "${version}",
	"name": "Mod Client or Server",
	"description": "This is an example description! Tell everyone what your mod is about!",
	"authors": [
		"Me!"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/mod-client-or-server/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.clientserver.ModClientOrServer"
		],
		"client": [
			"com.clientserver.ModClientOrServerClient"
		],
		"modmenu": [
			"com.clientserver.client.ModClientOrServerModMenu"
		],
		"fabric-datagen": [
			"com.clientserver.ModClientOrServerDataGenerator"
		]
	},
	"mixins": [
		"mod-client-or-server.mixins.json",
		{
			"config": "mod-client-or-server.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.18.4",
		"minecraft": "~1.21.1",
		"java": ">=21",
		"fabric-api": "*"
	}
}